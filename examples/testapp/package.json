{
    "include": [
        "*.html",
        {
            "type": "js",
            "files": [
                "components/*.js"
            ],
            "wrap": true,
            "wrapEach": true,
            "browser": "ie <9"
        },
        "main.js",
        {
            "type": "css",
            "files": [
                "*.css"
            ],
            "browser": "ie <9"
        },
        "less/*.less"
    ]
}